{
  "name": "s3-benchmark",
  "version": "1.0.0",
  "description": "S3 storage performance benchmarking tool",
  "main": "./src/main.ts",
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read --allow-sys src/main.ts",
    "dev": "deno run --allow-net --allow-env --allow-read --allow-sys --watch src/main.ts"
  },
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.0.0",
    "@std/cli": "jsr:@std/cli@^1.0.0",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0"
  }
}